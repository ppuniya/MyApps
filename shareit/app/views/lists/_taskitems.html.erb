<% if taskitems %>
	<div class="col-sm-offset-1 col-sm-1">
	<table id="newtaskitems" class="table table-striped table-hover">
		<thead>
			<tr>
			<th>Title</th>
			<th>Description</th>
			<th>Due Date</th>
			<th>Volunteer?</th>
			<tr>
		</thead>
		<tbody>
		<% taskitems.each do |it| %>
			<tr id="taskitem_<%=it.id%>" class="task_item">
				<%= render :partial=>"/taskitems/taskitem", :locals=>{:item=>it} %>
			</tr>
		<% end %>
		</tbody>
	</table>
	</div>
<% end %>
